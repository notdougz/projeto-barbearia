[flake8]
max-line-length = 88
extend-ignore = E203,W503,E501
exclude = 
    .git,
    __pycache__,
    venv,
    .venv,
    staticfiles,
    htmlcov,
    migrations,
    .github,
    node_modules,
per-file-ignores =
    __init__.py:F401
    migrations/*:E501
    settings.py:E501
max-complexity = 10
